@using Web.Areas.Admin.ViewModels.CoverVideo
@model CoverVideoCreateVM

<form enctype="multipart/form-data" asp-action="create" asp-controller="covervideo" method="post">
    <div class="form-group">
        <label asp-for="Url"></label>
        <input class="form-control" asp-for="Url" />
        <span asp-validation-for="Url"></span>
    </div>
    <div class="form-group">
        <label asp-for="CoverPhoto">Cover Photo</label>
        <input class="form-control" asp-for="CoverPhoto">
        <span asp-validation-for="CoverPhoto"></span>
    </div>

    <button type="submit">Create</button>
</form>
